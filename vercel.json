{
  "version": 2,
  "name": "antares-elite-platform",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://3.85.5.222/api/v1/$1",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "http://3.85.5.222/api/v1"
  },
  "functions": {
    "pages/**": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    },
    "src/app/**": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs"
}
